<div class="team__container">
    <div class="title-wrapper">
        {{!-- <img class="goitLogo" src="../images/goit.png" alt="goit"> --}}

        <a class="ref__readme" href="https://github.com/mityaua/filmoteka/blob/main/README.md" target="_blank" rel="noreferrer noopener"
            aria-label="Our Github">
            <img class="goitLogo" src="https://goit.ua/wp-content/themes/1/images/Layer.png" alt="GoIT" title="Our Github">
        </a>
        <h2 lang="en" class="team__title">TEAM<span class='under'>__</span>FOUR</h2>

        {{!-- --}}
    </div>

    <div class="team__content">
        <ul class="team__list list">
            {{#each this}}
            <li class="team__item">
                <div class="img__scale"><img src="{{imageMember}}" alt="{{name}}" class="team__image" /></div>

                {{!-- <div class="team__content"> --}}
                    <p lang="en" class="team__subtitle">{{name}}</p>
                    <p lang="en" class="team__text text">{{position}}</p>
                    <a class="social__link" href="{{socialLink}}" target="_blank" aria-label="ссылка Инстаграм">
                        {{!-- <svg class="social__icon">
                            <use href="../images/icons/sprite/svg#icon-github"></use>
                        </svg> --}}
                        <img class="social__icon"
                            src="data:image/png;base64,"
                            alt="icon github">
                        {{!-- <img class="social__icon" src="../images/git.png" alt="giticon"> --}}
                    </a>
            </li>
            {{/each}}
        </ul>
    </div>

    <button type=" button" class="close__button"></button>
</div>